{
  "name": "@ixinwu-ngp/web-core",
  "version": "1.1.4",
  "author": {
    "name": "zhuoluo-hq",
    "email": "zhuoluo.hq@gmail.com"
  },
  "homepage": "https://github.com/zhuoluo-hq/ngp-web/tree/master/packages/web-core",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.js",
  "module": "./src/index.js",
  "keywords": [
    "react",
    "reactjs",
    "react-redux",
    "redux",
    "redux-saga"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --root-mode upward src --out-dir build/lib",
    "build:es": "babel --root-mode upward src --out-dir build/es",
    "build:copy-files": "node ./scripts/copy-files.js",
    "clean": "rimraf build",
    "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:copy-files",
    "release": "yarn build && yarn publish build"
  },
  "peerDependencies": {
    "react": "^16.6.1",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  }
}
