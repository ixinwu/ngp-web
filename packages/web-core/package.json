{
  "name": "@ixinwu-ngp/web-core",
  "version": "1.1.3",
  "repository": "git@github.com:zhuoluo-hq/web-core.git",
  "author": {
    "name": "zhuoluo-hq",
    "email": "zhuoluo.hq@gmail.com"
  },
  "homepage": "https://github.com/zhuoluo-hq/web-core",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "files": [
    "lib",
    "src",
    "es"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-redux",
    "redux",
    "redux-saga"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "clean": "rimraf lib dist es coverage",
    "build": "npm run clean && npm run build:es && npm run build:commonjs"
  },
  "peerDependencies": {
    "react": "^16.6.1",
    "react-redux": "^5.1.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  }
}
