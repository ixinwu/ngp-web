  "repository": "git@github.com:zhuoluo-hq/web-core.git",
  "author": {
    "name": "zhuoluo-hq",
    "email": "zhuoluo.hq@gmail.com"
  },
  "homepage": "https://github.com/zhuoluo-hq/web-core",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "files": [
    "lib",
    "src",
    "es"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-redux",
    "redux",
    "redux-saga"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
