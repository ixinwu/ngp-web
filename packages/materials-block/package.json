{
  "name": "@ixinwu-ngp/materials-block",
  "version": "1.0.3",
  "author": {
    "name": "zhuoluo-hq",
    "email": "zhuoluo.hq@gmail.com"
  },
  "homepage": "https://github.com/zhuoluo-hq/fe-materials/tree/master/packages/ngp-materials-block",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel --config-file ../../babel.config.js ./src --out-dir ./build --ignore *.test.js",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel --config-file ../../babel.config.js ./src/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src --out-dir ./build/es --ignore *.test.js",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:copy-files": "babel-node --config-file ../../babel.config.js ./scripts/copy-files.js",
    "build": "yarn build:es2015 && yarn build:es2015modules && yarn build:es && yarn build:copy-files",
    "release": "yarn build && yarn publish build"
  },
  "peerDependencies": {
    "@ixinwu-ngp/materials-component": "^1.0.0",
    "@ixinwu-ngp/web-framework": "^1.0.0",
    "antd": "^3.10.8",
    "classnames": "^2.2.5",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1"
  },
  "dependencies": {
    "warning": "^4.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
