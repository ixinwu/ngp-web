{
  "name": "@ixinwu-ngp/web-framework",
  "version": "1.2.2",
  "repository": "git@github.com:zhuoluo-hq/web-framework.git",
  "author": {
    "name": "zhuoluo-hq",
    "email": "zhuoluo.hq@gmail.com"
  },
  "homepage": "https://github.com/zhuoluo-hq/ngp-web/tree/master/packages/web-framework",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.js",
  "module": "./src/index.js",
  "keywords": [
    "react",
    "reactjs",
    "react-redux",
    "redux",
    "redux-saga"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel --root-mode upward src --out-dir build/lib",
    "build:es": "babel --root-mode upward src --out-dir build/es",
    "build:copy-files": "node ./scripts/copy-files.js",
    "clean": "rimraf build",
    "build": "npm run clean && npm run build:es && npm run build:commonjs && npm run build:copy-files",
    "release": "yarn build && yarn publish build"
  },
  "peerDependencies": {
    "@ixinwu-ngp/materials-block": "^1.0.0",
    "@ixinwu-ngp/materials-component": "^1.0.0",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.6.2",
    "react-dom": "^16.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "dependencies": {
    "@ixinwu-ngp/web-core": "1.1.4",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.6.0",
    "react-click-outside": "^3.0.1",
    "react-jss": "^8.6.1",
    "warning": "^4.0.1"
  }
}
